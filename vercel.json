{
  "version": 2,
  "name": "riseedu-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_ENV": "production",
    "REACT_APP_API_URL": "https://api.riseedu.com/api"
  }
}